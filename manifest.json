{
	"manifest_version": 2,
	"name": "Show Battery2",
	"version": "1.0",
	"description": "This extension will show battery consumption.",
	"browser_action": {
		"default_icon": "sb2.png",
		"default_title": "消費電力を表示します"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"content_scripts": [
        {
        	"matches": [ "http://*/*", "https://*/*"],
		"match_about_blank": true,
        	"js": [
			"content_script.js",
			"jquery-2.2.3.min.js"
		],
		"run_at": "document_start"
        }
    ],
	"permissions": [
		"tabs"
	]
}
